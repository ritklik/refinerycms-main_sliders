<%= render 'refinery/admin/search_header', :url => refinery.main_sliders_admin_main_sliders_path %>
<div class='pagination_container'>
  <% if @main_sliders.any? %>
    <%= render 'main_sliders' %>
  <% else %>
    <p>
      <% unless searching? %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% else %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% end %>
    </p>
  <% end %>
</div>
